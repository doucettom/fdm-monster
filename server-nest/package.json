    "swagger-ui-express": "4.6.3",
    "typeorm": "0.3.16",
    "typeorm-extension": "2.8.0",
    "uuid": "9.0.0",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@nestjs/schematics": "10.0.2",
