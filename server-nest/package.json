{
  "name": "fdm-monster",
  "author": "David Zwart",
  "version": "2.0.0-alpha",
  "description": "FDM Monster is a bulk OctoPrint manager to set up, configure and monitor 3D printers. Our aim is to provide extremely optimized websocket performance and reliability.",
  "private": false,
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "cli": "nest start cli",
    "db:create": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create -s no",
    "db:drop": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:prettier": "prettier --write ./**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest.config.ts",
    "typeorm": "typeorm-ts-node-esm",
    "typeorm:generate": "yarn run build && typeorm-ts-node-esm.cmd migration:generate -d data-source.ts src/migrations/SomeMigration",
    "posttypeorm:generate": "prettier --write \"src/migrations/**/*.ts\"",
    "typeorm:migrate": "yarn run build && typeorm-ts-node-esm.cmd migration:run -d data-source.ts",
    "typeorm:revert": "typeorm-ts-node-esm.cmd migration:revert -d data-source.ts"
  },
  "dependencies": {
    "@automapper/classes": "8.7.7",
    "@automapper/core": "8.7.7",
    "@automapper/nestjs": "8.7.7",
    "@nestjs/axios": "3.0.0",
    "@nestjs/cache-manager": "2.1.0",
    "@nestjs/cli": "10.1.17",
    "@nestjs/common": "10.2.5",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.5",
    "@nestjs/event-emitter": "2.0.2",
    "@nestjs/jwt": "10.1.1",
    "@nestjs/passport": "10.0.2",
    "@nestjs/platform-express": "10.2.5",
    "@nestjs/platform-socket.io": "10.2.5",
    "@nestjs/platform-ws": "10.2.5",
    "@nestjs/schedule": "3.0.3",
    "@nestjs/serve-static": "4.0.0",
    "@nestjs/swagger": "7.1.11",
    "@nestjs/terminus": "10.0.1",
    "@nestjs/typeorm": "10.0.0",
    "@nestjs/websockets": "10.2.5",
    "@ntegral/nestjs-sentry": "4.0.0",
    "@sentry/node": "7.53.0",
    "@socket.io/admin-ui": "0.5.1",
    "adm-zip": "0.5.10",
    "axios": "1.4.0",
    "bcrypt": "5.1.0",
    "cache-manager": "5.2.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "got-cjs": "12.5.4",
    "helmet": "7.0.0",
    "ip": "1.1.8",
    "joi": "17.9.2",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "luxon": "3.3.0",
    "nest-commander": "3.7.1",
    "nest-winston": "1.9.2",
    "octokit": "2.0.18",
    "passport": "0.6.0",
    "pg": "8.11.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
