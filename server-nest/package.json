    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "json-to-typescript": "1.0.1",
    "prettier": "2.8.8",
